<!-- this hidden value carry the map subject from controller use it in javascript api call-->

<input type="hidden" id="areaID" value=@ViewBag.Area />





@{
    string _side_panel_path = "~/Views/Shared/_" + ViewBag.Area + "_side_panel.cshtml";

    Html.RenderPartial(_side_panel_path);

}




        <!-- Page Content -->
        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">@ViewBag.Area</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>



            <div class="row">

                <div class="col-lg-12">
                    <div class="panel panel-green">
                        <div class="panel-heading">
                           Base Map
                        </div>
                        <div class="panel-body">
                           
                                
                                <button type="button" class="btn btn-outline btn-success" onclick="location.href = '/localmap/localscrollerclienttablemap/county/cities'">Cities</button>
                            &nbsp;&nbsp;<button type="button" class="btn btn-outline btn-success" onclick="location.href = '/localmap/localsimplemap/county/rails'">Rails</button>
                               
                            &nbsp;&nbsp;<button disabled="disabled" type="button" class="btn btn-outline btn-default" onclick="location.href = '#'">Address</button>

                            &nbsp;&nbsp; <button disabled="disabled" type="button" class="btn btn-outline btn-default" onclick="location.href = '#'">Streets</button>

                               
                        </div>
                        <div class="panel-footer">

                        </div>
                    </div>
                </div>
                <!-- /.col-lg-4 -->
            </div>


            <!-- /.row -->
            <div class="row">

                <div class="col-lg-12">
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            Parks & Recreation Map
                        </div>
                        <div class="panel-body">
                          

                               

                                <button type="button" class="btn btn-outline btn-primary" onclick="location.href = '/localmap/localscrollerclienttablemap/county/parks'"> Parks</button>
                            &nbsp;&nbsp;<button type="button" class="btn btn-outline btn-primary" onclick="location.href = '/localmap/localsimplemap/county/water'">Water</button>
                                


                        </div>
                        <div class="panel-footer">

                        </div>
                    </div>
                </div>

            </div>



            <div class="row">

                <div class="col-lg-12">
                    <div class="panel panel-green">
                        <div class="panel-heading">
                            Education and Health
                        </div>
                        <div class="panel-body">
                           
                                <button type="button" class="btn btn-outline btn-info" onclick="location.href = '/localmap/localclusterscrollerclienttablemap/county/education_facility'">Education Facility</button>
                            &nbsp;&nbsp; <button type="button" class="btn btn-outline btn-info" onclick="location.href = '/localmap/localsimplemap/county/hospitals'">Hospitals</button>
                          
                        </div>
                        <div class="panel-footer">

                        </div>
                    </div>
                </div>



            </div>


            
            <div class="row">

                <div class="col-lg-12">
                    <div class="panel panel-green">
                        <div class="panel-heading">
                            School Education
                        </div>
                        <div class="panel-body">
                            
                                <button type="button" class="btn btn-outline btn-info" onclick="location.href = '/localmap/localclusterscrollerclienttablemap/county/fire_stations'">Fire station</button>

                            
                        </div>
                        <div class="panel-footer">

                        </div>
                    </div>
                </div>



            </div>



        

                </div>
                <!-- /#page-wrapper -->
                <!-- /#page-wrapper -->
          








            <!-- /#wrapper -->
        </div>


</body>
</html>